    {
      "cell_type": "code",
      "execution_count": 62,
      "metadata": {
        "id": "6fyO7MuhSvz0"
      },
      "outputs": [],
      "source": [
        "def compute_std(x):\n",
        "    return compute_variance(x) ** 0.5\n"
      ],
      "metadata": {
