        "def compute_variance(x):\n",
        "    mean = compute_mean(x)\n",
        "    return sum((xi - mean)**2 for xi in x) / (len(x) - 1)\n"
      ],
      "metadata": {
        "id": "-lpXq_sdSutO"
      },
      "execution_count": null,
      "outputs": []
      ]
