      "source": [
        "def compute_median(x):\n",
        "    x_sorted = sorted(x)\n",
        "    n = len(x_sorted)\n",
        "    mid = n // 2\n",
        "    if n % 2 == 0:\n",
        "        return (x_sorted[mid - 1] + x_sorted[mid]) / 2\n",
        "    else:\n",
        "        return x_sorted[mid]\n"
