    {
      "cell_type": "code",
      "outputs": [],
      "source": [
        "def min_value(x):\n",
        "    m = x[0]\n",
        "    for value in x:\n",
        "        if value < m:\n",
        "            m = value\n",
        "    return m\n",
        "\n",
        "def max_value(x):\n",
        "    m = x[0]\n",
        "    for value in x:\n",
        "        if value > m:\n",
        "            m = value\n",
        "    return m\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 64,
      "metadata": {
        "id": "eJHlk6Z8Txpy"
      },
      "execution_count": null,
      "outputs": []
      "outputs": [],
      "source": [
        "def compute_mode(x):\n",
        "    freq = {}\n",
        "    for value in x:\n",
        "        freq[value] = freq.get(value, 0) + 1\n",
        "    max_freq = max_value(list(freq.values()))\n",
        "    modes = [k for k, v in freq.items() if v == max_freq]\n",
        "    return modes[0]  # return first mode\n"
      ]
    },
