      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "#import seaborn as sns\n",
        "\n",
        "# 1. Load the dataset\n",
        "df = pd.read_csv(\"Dataset/Life Expectancy Data.csv\")\n",
        "\n",
        "# 2. Clean column names (Remove annoying extra spaces)\n",
        "df.columns = df.columns.str.strip()\n",
        "\n",
        "# 3. Calculate the Correlation Coefficient (Pearson)\n",
        "# We use 'drop' to ignore the NaNs for this specific calculation\n",
        "correlation = df['under-five deaths'].corr(df['Life expectancy'])\n",
        "\n",
        "print(f\"The Correlation between under-five deaths and Life Expectancy is: {correlation:.4f}\")\n",
        "\n",
        "# 4. (Optional) Visual Proof - Scatter Plot\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.scatter(x=df['under-five deaths'], y=df['Life expectancy'])\n",
        "plt.title(f\"under-five deaths vs Life Expectancy (r = {correlation:.2f})\")\n",
        "plt.xlabel(\"under-five deaths\")\n",
        "plt.ylabel(\"Life Expectancy\")\n",
        "plt.grid(True)\n",
        "plt.show()"
      ]
    },
