        "# 1. Define lists for different treatments\n",
        "cols_to_log = ['GDP', 'percentage expenditure', 'Measles', 'HIV/AIDS', 'infant deaths', 'under-five deaths', 'Population']\n",
        "cols_to_cap = ['Income composition of resources', 'thinness 5-9 years', 'Total expenditure', 'Hepatitis B', 'Polio', 'Diphtheria', 'Schooling', 'Alcohol', 'thinness 1-19 years']\n",
        "cols_to_drop_rows = ['Life expectancy', 'Adult Mortality']\n",
        "\n",
        "# STRATEGY 1: Log Transformation (To consider the differences between outliers)\n",
        "for col in cols_to_log:\n",
        "    data[col] = np.log1p(data[col])  # Use log1p to avoide log(0)\n",
        "\n",
        "# STRATEGY 2: Capping (Handles \"Noisy\" Outliers)\n",
        "for col in cols_to_cap:\n",
        "    Q1 = data[col].quantile(0.25)\n",
        "    Q3 = data[col].quantile(0.75)\n",
        "    IQR = Q3 - Q1\n",
        "    lower = Q1 - 1.5 * IQR\n",
        "    upper = Q3 + 1.5 * IQR\n",
        "    \n",
        "    # Apply Capping\n",
        "    data[col] = np.where(data[col] < lower, lower, data[col])\n",
        "    data[col] = np.where(data[col] > upper, upper, data[col])\n",
        "\n",
        "# STRATEGY 3: Dropping Rows (Handles Data Errors)\n",
        "for col in cols_to_drop_rows:\n",
        "    Q1 = data[col].quantile(0.25)\n",
        "    Q3 = data[col].quantile(0.75)\n",
        "    IQR = Q3 - Q1\n",
        "    lower = Q1 - 1.5 * IQR\n",
        "    upper = Q3 + 1.5 * IQR\n",
        "\n",
        "    # Delete rows\n",
        "    data = data[(data[col] > lower) & (data[col] < upper)]"
      ]
